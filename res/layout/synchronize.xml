<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:padding="5dp"
	>
	<TextView android:id="@+id/syntitle"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:gravity="center_horizontal"
		android:text="@string/syntype"
		android:textSize="25sp"
		android:textColor="#8C6931"
	/>
	<Button android:id="@+id/btnsynupload"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_below="@id/syntitle"
		android:text="@string/synupload"
	/>
	<Button android:id="@+id/btnsyndownload"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_below="@id/btnsynupload"
		android:text="@string/syndownload"
	/>
	<Button android:id="@+id/btnsynback"
		android:layout_width="90dp"
		android:layout_height="wrap_content"
		android:layout_below="@id/btnsyndownload"
		android:layout_alignParentRight="true"
		android:text="@string/back"
	/>
</RelativeLayout>