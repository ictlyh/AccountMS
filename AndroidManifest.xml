<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ac.ucas.accountmanagementsystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>
 	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
 	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
 	
    <application android:icon="@drawable/appicon" android:label="@string/app_name" android:allowBackup="true">
        <activity android:name="ac.ucas.accountmanagement.activity.Login"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ac.ucas.accountmanagement.activity.MainActivity"
                  android:label="@string/app_name">
        </activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.AddOutAccount"
			android:label="@string/addoutaccount">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.AddInAccount"
			android:label="@string/addinaccount">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.OutAccountInfo"
			android:label="@string/outaccountinfo">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.InAccountInfo"
			android:label="@string/inaccountinfo">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.ShowInfo"
			android:label="@string/showinfo">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.Sysset"
			android:label="@string/sysset">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.AccountFlag"
			android:label="@string/accountflag">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.InfoManagement"
			android:label="@string/infomanage">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.FlagManagement"
			android:label="@string/flagmanage">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.ChangePwd"
			android:label="@string/sysset">
		</activity>
		<activity  android:name="ac.ucas.accountmanagement.activity.Synchronize"
			android:label="@string/synchronize">
		</activity>
    </application>

</manifest>
