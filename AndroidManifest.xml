<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ac.ucas.accountmanagementsystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application android:icon="@drawable/appicon" android:label="@string/app_name" android:allowBackup="true">
        <activity android:name=".Login"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
                  android:label="@string/app_name">
        </activity>
		<activity  android:name=".AddOutAccount"
			android:label="@string/addoutaccount">
		</activity>
		<activity  android:name=".AddInAccount"
			android:label="@string/addinaccount">
		</activity>
		<activity  android:name=".OutAccountInfo"
			android:label="@string/outaccountinfo">
		</activity>
		<activity  android:name=".InAccountInfo"
			android:label="@string/inaccountinfo">
		</activity>
		<activity  android:name=".ShowInfo"
			android:label="@string/showinfo">
		</activity>
		<activity  android:name=".Sysset"
			android:label="@string/sysset">
		</activity>
		<activity  android:name=".AccountFlag"
			android:label="@string/accountflag">
		</activity>
		<activity  android:name=".InfoManagement"
			android:label="@string/infomanage">
		</activity>
		<activity  android:name=".FlagManagement"
			android:label="@string/flagmanage">
		</activity>
    </application>

</manifest>
